{
    "name": "kasoft/yii2-fileupload",
    "description": "FileUpload is plugin for uploading and handling Files in Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Menrad Kasoft",
            "email": "nils@menrad.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "~2.0.*",
        "npm-asset/filepond": "^4.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": {
            "kasoft\\fileupload\\": ""
        }
    }
}
